<div id="unload">
		{{#if saved}}
			{{#each saved}}
				<div class="panel entryWrapper" data-id="{{this.id}}">
					<div class="panel-heading entryHeader">
						 <h3 class='panel-title entryTitle'>{{this.title}}</h3>
					</div>
					<div class="panel-body entryBody">
						{{this.link}}
						<button type="button" data-id="{{this.id}}" id="deleteArticle" class="btn btn-danger">Delete Article</button>
						<button type="button" data-id="{{this.id}}" id="addNoteButton" class="btn btn-primary">Add Note</button>
					</div>
				</div>
			{{/each}}
		{{/if}}

	

	<!-- Modal -->
	<div id="addNote" class="modal fade" role="dialog">
	  <div class="modal-dialog">

	    <!-- Modal content-->
	    <div class="modal-content">
	     	<div class="modal-header">
	        	<button type="button" class="close" data-dismiss="modal">&times;</button>
	        	<h3 class="modal-title" id="modal-title">Note for <span id="articleID"> </span></h3>
	     	</div>
	    	<div class="modal-body" id="dropArticleNotes">
	    		
	    	</div>
	    	<textarea class="noteText" id="bodyinput" rows="5" cols="75" name="body" value="Enter Note Here"></textarea><br>
	    	<div class="modal-footer">
	      		<div id="submitbuttonhere">
	      		</div>
	      </div>
	    </div>

	  </div>
	</div>



</div>
